<?xml version="1.0" encoding="utf-8"?>
<nodeList xmlns:its="http://www.w3.org/2005/11/its"
          xmlns:datc="http://example.com/datacats">
   <nodeList datacat="localefilter">
      <node path="/book" outputType="no-value">
         <output/>
      </node>
      <node path="/book/info[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/book/info[1]/its:span[1]" outputType="new-value-local">
         <output localeFilterList="en-CA, fr-CA"/>
      </node>
      <node path="/book/info[1]/its:span[1]/@localeFilterList"
            outputType="inherited">
         <output localeFilterList="en-CA, fr-CA"/>
      </node>
      <node path="/book/info[1]/its:span[1]/para[1]" outputType="inherited">
         <output localeFilterList="en-CA, fr-CA"/>
      </node>
   </nodeList>
</nodeList>
