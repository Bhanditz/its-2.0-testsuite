<?xml version="1.0" encoding="utf-8"?>
<nodeList xmlns:its="http://www.w3.org/2005/11/its"
          xmlns:datc="http://example.com/datacats">
   <nodeList datacat="idvalue">
      <node path="/html" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/title[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/link[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/link[1]/@href" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/link[1]/@rel" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/p[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/p[1]/@id" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/p[1]/b[1]" outputType="new-value-global">
         <output idValue="concat(../@id, '_t')"/>
      </node>
      <node path="/html/body[1]/p[1]/b[1]/@id" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/p[1]/var[1]" outputType="new-value-global">
         <output idValue="concat(../@id, '_d')"/>
      </node>
      <node path="/html/body[1]/p[1]/var[1]/@id" outputType="no-value">
         <output/>
      </node>
   </nodeList>
</nodeList>
