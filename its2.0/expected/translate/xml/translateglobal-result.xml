      <node path="/myDoc/head[1]" outputType="new-value-global">
         <output translate="no"/>
      </node>
      <node path="/myDoc/head[1]/its:rules[1]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/myDoc/head[1]/its:rules[1]/@version" outputType="default-value">
