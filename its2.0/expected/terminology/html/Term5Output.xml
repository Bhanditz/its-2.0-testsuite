<?xml version="1.0" encoding="utf-8"?>
<nodeList xmlns:its="http://www.w3.org/2005/11/its"
          xmlns:datc="http://example.com/datacats">
   <nodeList datacat="terminology">
      <node path="/html" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/@lang" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/head[1]" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/head[1]/meta[1]" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/head[1]/meta[1]/@charset" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/head[1]/title[1]" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/body[1]" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/body[1]/p[1]" outputType="default-value">
         <output its:term="no"/>
      </node>
      <node path="/html/body[1]/p[1]/span[1]" outputType="new-value-local">
         <output term="yes"/>
      </node>
      <node path="/html/body[1]/p[1]/span[1]/@its-term" outputType="default-value">
         <output its:term="no"/>
      </node>
   </nodeList>
</nodeList>
