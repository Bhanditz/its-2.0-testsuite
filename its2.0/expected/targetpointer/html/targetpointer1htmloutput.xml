<?xml version="1.0" encoding="utf-8"?>
<nodeList xmlns:its="http://www.w3.org/2005/11/its"
          xmlns:datc="http://example.com/datacats">
   <nodeList datacat="translate">
      <node path="/html" outputType="new-value-global">
         <output translate="no"/>
      </node>
      <node path="/html/head[1]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/html/head[1]/title[1]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/html/head[1]/link[1]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/html/head[1]/link[1]/@href" outputType="default-value">
         <output its:translate="no"/>
      </node>
      <node path="/html/head[1]/link[1]/@rel" outputType="default-value">
         <output its:translate="no"/>
      </node>
      <node path="/html/body[1]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/html/body[1]/div[1]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/html/body[1]/div[1]/@id" outputType="default-value">
         <output its:translate="no"/>
      </node>
      <node path="/html/body[1]/div[1]/h4[1]" outputType="new-value-global">
         <output translate="yes"/>
      </node>
      <node path="/html/body[1]/div[1]/b[1]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/html/body[1]/div[2]" outputType="inherited">
         <output translate="no"/>
      </node>
      <node path="/html/body[1]/div[2]/@id" outputType="default-value">
         <output its:translate="no"/>
      </node>
      <node path="/html/body[1]/div[2]/h4[1]" outputType="new-value-global">
         <output translate="yes"/>
      </node>
      <node path="/html/body[1]/div[2]/b[1]" outputType="inherited">
         <output translate="no"/>
      </node>
   </nodeList>
   <nodeList datacat="targetpointer">
      <node path="/html" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/title[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/link[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/link[1]/@href" outputType="no-value">
         <output/>
      </node>
      <node path="/html/head[1]/link[1]/@rel" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/div[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/div[1]/@id" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/div[1]/h4[1]" outputType="new-value-global">
         <output>
            <targetPointer>
               <b>lastFolder</b>
            </targetPointer>
         </output>
      </node>
      <node path="/html/body[1]/div[1]/b[1]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/div[2]" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/div[2]/@id" outputType="no-value">
         <output/>
      </node>
      <node path="/html/body[1]/div[2]/h4[1]" outputType="new-value-global">
         <output>
            <targetPointer>
               <b>testFile.txt</b>
            </targetPointer>
         </output>
      </node>
      <node path="/html/body[1]/div[2]/b[1]" outputType="no-value">
         <output/>
      </node>
   </nodeList>
</nodeList>
